 <mvc:View controllerName="com.sap.mydemo.controller.App"
  displayBlock="true"
  xmlns="sap.m"
  xmlns:mvc="sap.ui.core.mvc">
  <App id="idAppControl" >
    <pages>
      <Page title="{i18n>title}">
        <content>
          <List items="{/Products}">
            <StandardListItem title="{ProductName}">
            </StandardListItem>
          </List>
        </content>
      </Page>
    </pages>
  </App>
</mvc:View>
